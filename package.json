{
  "name": "rivet-plugin-pdf2md",
  "packageManager": "yarn@3.5.0",
  "version": "0.0.4",
  "type": "module",
  "main": "dist/bundle.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b && tsx bundle.ts",
    "dev": "run-p watch:tsc watch:esbuild:sync",
    "watch:tsc": "tsc -b -w --preserveWatchOutput",
    "watch:esbuild": "tsx bundle.ts --watch",
    "watch:esbuild:sync": "tsx bundle.ts --watch --sync",
    "postinstall": "npm install canvas @opendocsg/pdf2md node-html-markdown"
  },
  "dependencies": {
    "@ironclad/rivet-core": "latest",
    "@opendocsg/pdf2md": "^0.1.28",
    "canvas": "^2.11.2",
    "node-html-markdown": "^1.3.0"
  },
  "devDependencies": {
    "esbuild": "^0.19.2",
    "npm-run-all": "^4.1.5",
    "recursive-copy": "^2.0.14",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2"
  },
  "volta": {
    "node": "20.6.1"
  },
  "rivet": {
    "skipInstall": true
  }
}
